<?xml version="1.0"  encoding="ISO-8859-1"?>

<pingus-level>
  <global>
    <levelname lang="de">Unerreichbarer Ausgange im Slime</levelname>
    <levelname lang="en">Unreachable exit in slime</levelname>

    <description lang="de">Die Pingus scheinen nun in der Hölle
gelandet zu sein, um sie herum Lava und Fallen. Der Ausgang scheint
unerreichbar. Doch mit gezieltem Graben sollten sie noch eine Chance
haben.</description>

    <description lang="en">The pingus seems to be in hell, surounded
    by lava and the exit unreachable. When they all hold together they
    might be able to escape.</description>

    <author>Ingo Ruhnke &lt;grumbel@gmx.de&gt;</author>
    <number-of-pingus>100</number-of-pingus>
    <number-to-save>90</number-to-save>
    <time>-1</time>
    <width>1728</width>
    <height>864</height>
    <playable>1</playable>
    <difficulty>60</difficulty>
  </global>

  <action-list>
    <basher count="10"/>
    <blocker count="10"/>
    <bomber count="10"/>
    <bridger count="20"/>
    <digger count="10"/>
    <miner count="10"/>
  </action-list>


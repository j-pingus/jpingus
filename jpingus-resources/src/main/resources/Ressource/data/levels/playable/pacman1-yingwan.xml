<?xml version="1.0"  encoding="ISO-8859-1"?>

<pingus-level>
  <global>
    <levelname lang="en">Pacman 1</levelname>
    <description lang="en">Get to Mr. Pacman and avoid all the nasty ghosties to bring the
Pingus home.</description>
    <author>Ying-Wan Lam &lt;yingwan.lam@yale.edu&gt;</author>
    <number-of-pingus>50</number-of-pingus>
    <number-to-save>40</number-to-save>
    <time>9000</time>
    <width>800</width>
    <height>600</height>
  </global>


<?xml version="1.0"?>

<pingus-level>
  <global>
    <levelname lang="en">Sandstorm</levelname>
    <description lang="en">Deep inside the pyrmaid the pingus are surprised to find their way
blocked by a large sand drift - pondering the absence of gravity they continue
relentlessly...</description>
    <author>Tom Flavel &lt;tomflavel@yahoo.co.uk&gt;</author>
    <number-of-pingus>80</number-of-pingus>
    <number-to-save>70</number-to-save>
    <time>9000</time>
    <width>1600</width>
    <height>800</height>
    <comment>suffers solid-groundpiece confusion</comment>
  </global>

<background>
  <surface><resource type="datafile">
    <resource-datafile>textures</resource-datafile>
